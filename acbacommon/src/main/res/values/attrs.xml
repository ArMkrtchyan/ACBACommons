<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="errorMessage" format="string" />
    <attr name="isPreventClick" format="boolean" />

    <declare-styleable name="EditTextACBA">
        <attr name="validator" format="enum">
            <enum name="none" value="0" />
            <enum name="email" value="1" />
            <enum name="name" value="2" />
            <enum name="password" value="3" />
            <enum name="regex" value="4" />
        </attr>
        <attr name="errorMessage" />
        <attr name="regex" format="string" />
        <attr name="minLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="TextInputLayoutACBA"></declare-styleable>

    <declare-styleable name="ButtonACBA">
        <attr name="clickInterval" format="integer" />
        <attr name="isPreventClick" />
    </declare-styleable>

    <declare-styleable name="CheckBoxACBA">
        <attr name="errorMessage" />
        <attr name="mustBeChecked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ToolbarACBA"></declare-styleable>
    <declare-styleable name="TextViewACBA"></declare-styleable>
    <declare-styleable name="ImageViewACBA"></declare-styleable>

</resources>